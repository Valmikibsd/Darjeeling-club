@{
    ViewData["Title"] = "BookingReports";
    Layout = "~/Areas/Admin/Views/Shared/_MainLayout.cshtml";
}
<style>
    .on {
        background-color: red;
    }

    #mytbl td {
        cursor: pointer;
    }

    .Card {
        display: inline-block;
        width: 333px;
    }
</style>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
<div class="container-fluid">
    <br>
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Availability</h6>
        </div>
        <div class="card-body">
            <div class="row">
            </div>
            <div class="row">
                <input type="hidden" id="id" value="0" />

                <div class="col-md-4">
                    <div class="form-group">
                        <label class="font-weight-700 font-size-16">From Date</label>
                        <input type="date" class="form-control" id="txtdate1" onchange="ShowDataInTable()" />
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                        <label class="font-weight-700 font-size-16">To Date</label>
                        <input type="date" class="form-control" id="txtdate2" onchange="ShowDataInTable()" />
                    </div>
                </div>

                <div class="form-actions mt-10" style="display:none">
                    <input type="button" class="btn btn-primary" id="btnsave" onclick="ShowDataInTable()"
                           value="Show" />
                </div>
                <div class="col-md-12" id="Printdiv">
                </div>
                <div id="venudiv" class="container">
                </div>
            </div>
        </div>
    </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.3/dist/jquery.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="~/js/Todaydate_NowTime.js"></script>
@*<script src="~/js/VenueReport.js"></script>*@
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<script type="text/javascript">
    $(document).ready(function () {
        datebefore1month = datebefore1month();
        console.log(datebefore1month);
        todaydate = Todaydate();
        $("#txtdate2").val(todaydate);
        $("#txtdate1").val(todaydate);
        ShowDataInTable()
    })
    function ShowDataInTable() {
        $.ajax({
            url: '/Admin/ClubApp/IUBookingReports',
            data: {},
            type: 'post',
            datatype: 'json',
            success: function (data) {
                console.log(data);
                data = JSON.parse(data);

                var row = "<table id='data-table' class='table table-bordered table-hover w-100 dataTable dtr-inline'><tr id='mytbl'>";
                for (var i = 0; i < data.length; i++) {
                    //if (data[i].class != null) {
                    //	varclass = data[i].class
                    //}
                    //row += "<td id='test" + i.toString() + "' onclick='roundWise(" + i.toString() + ")' >" + data[i].VOTE_ROUND + "</td>";
                    row += "<td style='display:none;' id='id" + i.toString() + "'  onclick='roundWise(" + i.toString() + ")' >" + data[i].ID + "</td><td id='test" + i.toString() + "'  onclick='roundWise(" + i.toString() + ")' >" + data[i].VenueName + "</td>";
                }
                row += "</tr></table>";
                $("#Printdiv").html(row);


            }, error: function (er) {
                alert(er);
            }
        });
    }

    function roundWise(i) {
        var colctr = $("#data-table tr td").length;
        for (var k = 0; k < colctr; k++) {
            $("#test" + k + "").removeClass('on');
        }
        $("#test" + i + "").addClass('on');
        //var round=$("#rnd" + i + "").text();
        //alert(round);
        var VenueId = $("#id" + i + "").text();

        //var data = { Stateid: Stateid, ac: ac, Round: x }
        //getDatagd(url, data, "", "", "");
        BindVenueData(VenueId);
    }

    function BindVenueData(VenueId) {
        $.ajax({
            url: '/Admin/ClubApp/BindVenueData',
            dataType: 'JSON',
            data: { VenueId: VenueId, VENUE_DATE1: $("#txtdate1").val(), VENUE_DATE2: $("#txtdate2").val() },
            type: 'POST',
            //contentType: 'application/json; charset=utf-8',
            success: function (data) {
                //console.log(data);
                $("#venudiv").html(data);
                //$('.SubCategorybody').html(html);
            }
        });
    }
</script>