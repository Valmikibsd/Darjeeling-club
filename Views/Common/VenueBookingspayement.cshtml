</head>

<body class="color-theme-blue">
    <div class="wrapper">
        <!-- page main start -->
        <div class="page">
            <div class="page-content h-100">
                <div class="page-content">
                    <div class="content-sticky-footer pt-2">

                        <div class="col-12 mb-4">
                            <div class="row">
                                <div class="col-12"><a href="/Common/home" class="menu-left-close inner_back block-title"><i class="material-icons">keyboard_arrow_left</i></a><h2 class="block-title inner_title title_top">Venue Bookings</h2></div>
                            </div>
                            <div class="row">
                                @foreach (var sub in Model.venuebookings)
                                {
                                    <div class="col-6 col-md-4 col-lg-2" style="pointer-events: none;">
                                        <div class="card venue opendalog" data-toggle="modal" data-target="#colorscheme" data-id="@sub.Id" data-name="@sub.name">
                                            <div class="card-body text-center">
                                                <div class="w-100">
                                                    <h4 class="font-weight mb-0">@sub.name</h4>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }

                            </div>
                            @using (Html.BeginForm("Complete", "Common"))
                            {
                                @Html.AntiForgeryToken()

                             
                                <input type="hidden" id="txttok" name="txttok" value="@ViewBag.tokenid" />
                                <input type="hidden" id="emailid" name="emailid" value="@ViewBag.EMAIL" />
                                <input type="hidden" id="mobileno" name="mobileno" value="@ViewBag.MOBILE" />
                                <a href="#"  style="display:none" onclick="openPay();" id="rzp-button1" return false">pay</a>
                                <button type="submit" id="rzp-paymentresponse" class="btn btn-primary" hidden>Submit</button>
                                 <a href="/Common/VenueBookings" id="back" class="btn login_btn border-0 z-3">Back</a>
                            }
                            
                            </div>
                        </div>

                    </div>
                </div>

                <br>

            </div>

        </div>
      
</body>

</html>
